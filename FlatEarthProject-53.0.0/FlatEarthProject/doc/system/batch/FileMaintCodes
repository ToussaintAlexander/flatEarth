package com.external.association.specifications.value.model;

public class FileMaintCodes {
	
	private String tag;
	private int length;
	private String value;
	
	public FileMaintCodes(String tag, int length, String value) {
		super();
		this.tag = tag;
		this.length = length;
		this.value = value;
	}
	
	public String getTag() {
		return tag;
	}
	public void setTag(String tag) {
		this.tag = tag;
	}
	public int getLength() {
		return length;
	}
	public void setLength(int length) {
		this.length = length;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	
}
