<%@ page autoFlush = "true" %>							<!-- Controls the behavior of the servlet output buffer. -->
<%@ page buffer = "8kb" %>								<!-- Specifies a buffering model for the output stream. -->
<%@ page contentType = "text/xml" %>					<!-- Defines the character encoding scheme. -->
<%@ page errorPage = "false" %>							<!-- Defines the URL of another JSP that reports on Java unchecked runtime exceptions. -->
<%@ page isErrorPage = "false" %>						<!-- Indicates if this JSP page is a URL specified by another JSP page's errorPage attribute. -->
<%@ page info = "This JSP Page Written By ZARA"  %>		<!-- Defines a string that can be accessed with the servlet's getServletInfo() method. -->
<%@ page isThreadSafe="true"  %>						<!-- JSP container may dispatch multiple outstanding client requests to the page simultaneously. -->
<%@ page language = "java" %>							<!-- Defines the programming language used in the JSP page. -->